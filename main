#include <iostream>
using namespace std;

int main()
{
	double num1, num2 , fact_1 = 1 , fact_2 =1;
		
	while (cin >> num1 && cin >> num2)
	{
		for (int i = 1; i <= num1; i++)
		{
			fact_1 = fact_1*i;
		}


		for (int x = 1; x <= num2; x++)
		{
			fact_2 = fact_2*x;
		}


		cout << fact_1 + fact_2 << endl;
	}
	system("pause");
		return 0;
}
